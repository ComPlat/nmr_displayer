{
  "name": "NMRiumWrapper",
  "version": "0.1.1",
  "private": false,
  "license": "MIT",
  "main": "lib-cjs/NMRiumWrapper.js",
  "module": "lib/NMRiumWrapper.js",
  "files": [
    "lib",
    "lib-cjs"
  ],
  "volta": {
    "node": "16.15.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
